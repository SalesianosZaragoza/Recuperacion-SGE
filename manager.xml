<odoo>
    
    <menuitem name="Park manager" id="manager_menu_root"/>

    <record id ="model_access_todo" model="ir.access.model">
        <field name="name">manager.access.todo</field>
        <field name="model_id" ref="manager.park"/>
        <field name="group_id" ref=""/>
        <field name="perm_read" eval="1"/>
        <field name="perm_write" eval="1"/>
        <field name="perm_create" eval="1"/>
        <field name="perm_unlink" eval="1"/>
       </record>

       
    <inform 
    id = "account_invoices" 
    model = "account.invoice" 
    string = "Invoices" 
    report_type = "Qweb-pdf" 
    name = "account.report_invoice" 
    file = "account.report_invoice" 
    attachment_use = "True" 
    attached = "(objet .state en ('open', 'paid')) y 
        ('INV' + (object.number o '') .replace ('/', '') + '. pdf') " />  

<menuitem id="manager_menu_inform" name="Inform"  parent="manager_menu_root"/>

    <menuitem name="invoices" id="manager_list_invoice" parent="manager_menu_invoice" 
              action="invoice_list_action"/>

       
</odoo>
