<odoo>
    <record model="ir.ui.view" id="managment_form_view">
        <field name="name">managment.form</field>
        <field name="model">npi.managment</field>
        <field name="arch" type="xml">
            <form string="Managment Form">
                <sheet>
                    <group>
                        <field name="dni"/>
                        <field name="ss"/>
                        <field name="name"/>
                        <field name="adress"/>
                        <field name="mobile_phone"/>
                        <field name="landline"/>
                        <field name="salary"/>
                        <field name="naturalPark_id"/>
                        <field name="entry"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record model="ir.ui.view" id="managment_tree_view">
        <field name="name">managment.tree</field>
        <field name="model">npi.managment</field>
        <field name="arch" type="xml">
            <tree string="Managment Tree">
                <field name="dni"/>
                <field name="ss"/>
                <field name="name"/>
                <field name="adress"/>
                <field name="mobile_phone"/>
                <field name="landline"/>
                <field name="salary"/>
                <field name="naturalPark_id"/>
                <field name="entry"/>
            </tree>
        </field>
    </record>

    <record model="ir.actions.act_window" id="managment_list_action">
        <field name="name">Managment</field>
        <field name="res_model">npi.managment</field>
        <field name="view_mode">tree,form</field>
        <field name="help" type="html">
            <p class="o_view_nocontent_smiling_face">Insert a managment</p>              
        </field>
    </record>



