<odoo>

        <record model="ir.ui.view" id="natural_park_form_view">
            <field name="name">natural_park.form</field>
            <field name="model">ges.natural_park</field>
            <field name="arch" type="xml">
                <form string="Natural Park Form">
                    <sheet>
                        <group>
                            <field name="name"/>
                            <field name="starting_date"/>
                            <field name="autonomous_community_natural_park_ids"/>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

        <record model="ir.ui.view" id="natural_park_tree_view">
            <field name="name">natural_park.tree</field>
            <field name="model">ges.natural_park</field>
            <field name="arch" type="xml">
                <tree string="Natural Park Tree">
                    <field name="name"/>
                    <field name="starting_date"/>
                    <field name="autonomous_community_natural_park_ids"/>
                </tree>
            </field>
        </record>

        <record model="ir.actions.act_window" id="natural_park_list_action">
            <field name="name">Natural Park</field>
            <field name="res_model">ges.natural_park</field>
            <field name="view_mode">tree,form</field>
            <field name="help" type="html">
            </field>
        </record>
        
        <menuitem id="main_natural_parks_menu" name="natural_parks"/>

        <menuitem id="natural_parks_menu" name="natural_parks"
                  parent="main_natural_parks_menu"/>
        
        <menuitem id="natural_park_menu" name="Natural Park"
            parent="natural_parks_menu"
            action="natural_park_list_action"/>

</odoo>